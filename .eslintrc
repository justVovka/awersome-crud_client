{
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "parser": "@typescript-eslint/parser",
  "rules": {
    "prettier/prettier": "error",
    "no-console": "error",
    "no-debugger": "error",
    "eqeqeq": "error",
    "no-unused-vars": "error",
    "no-unused-expressions": ["error", { "allowShortCircuit": true }]
  }
}
